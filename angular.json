{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-loading": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/agencycoda/mia-loading",
      "sourceRoot": "projects/agencycoda/mia-loading/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/agencycoda/mia-loading/tsconfig.lib.json",
            "project": "projects/agencycoda/mia-loading/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-loading/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-loading/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-loading/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-loading/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/agencycoda/mia-loading/tsconfig.lib.json",
              "projects/agencycoda/mia-loading/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "2a3d92ff-fcec-443c-847d-99a0e004868d"
  },
  "defaultProject": "@agencycoda/mia-loading"
}